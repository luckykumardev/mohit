    <script>
        function report() {
            let region = document.getElementById("body"); // whole screen
            html2canvas(region, {
                onrendered: function (canvas) {
                pngUrl = canvas.toDataURL();
                img = document.querySelector(".screen");
                img.src = pngUrl;
                 
                 console.log(img);
 

                },
            });
        }      
    </script>




